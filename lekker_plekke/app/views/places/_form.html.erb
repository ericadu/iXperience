<div class="col-md-6">
<%= form_for @place do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </p>

  <p class="form-group">
    <%= f.label :address %>
    <%= f.text_field :address, class: "form-control"  %>
  </p>

  <p class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control"  %>
  </p>

  <p class="form-group">
    <%= f.label :image %>
    <%= f.text_field :image, class: "form-control"  %>
  </p>

  <p class="form-group">
    <%= f.label :rating %>
    <%= f.number_field :rating, in: 1..5, class: "form-control"  %>
  </p>

  <p>
    <%= f.submit class: "btn btn-default" %>
  </p>

<% end %>
</div>